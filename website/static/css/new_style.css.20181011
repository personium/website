/* ページトップ */
#page-top {
    position: fixed;
    bottom: 20px;
    right: 20px;
    font-size: 80%;
	opacity: 0.5;
}

#page-top a:before {
    content: '';
    position: absolute;
    top: 45%;
    left: 0;
    right: 0;
    width: 12px;
    height: 12px;
    margin: auto;
    border: 1px solid #fff;
    border-color: #fff #fff transparent transparent;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

#page-top a {
    background: #575757;
    text-decoration: none;
    color: #fff;
    width: 60px;
    padding: 30px 0;
    text-align: center;
    display: block;
    border-radius: 30px;
}



#page-top a:hover {
    text-decoration: none;
    background: #999;
}



