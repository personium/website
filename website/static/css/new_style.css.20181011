.nav-menu.menu-is-open {
    background-color: rgba(22, 44, 22, 0.95);
    box-shadow: 0px 5px 23px 0px rgba(0, 0, 0, 0.1);
}
	
}

li.sns i{
    font-size: 1.6rem;
    vertical-align: middle;
}

@media (min-width: 992px) {
    li.sns i{
        color: #ffffff;
    }
