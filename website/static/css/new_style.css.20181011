/* GLOBAL
----------------------*/

body {
    font-family: 'Rubik', sans-serif;
    position: relative;
}

a {
    color: #1ebe8c;
}

a:hover,
a:focus {
    color: #1ebe8c;
}

h1 {
    font-size: 60px;
    font-weight: 300;
    letter-spacing: -1px;
    margin-bottom: 1.5rem;
}

h2 {
    font-size: 45px;
    font-weight: 300;
    color: #1ebe8c;
    letter-spacing: -1px;
    margin-bottom: 1rem;
}

h3 {
    color: #1ebe8c;
    font-size: 33px;
    font-weight: 500;
}

h4 {
    font-size: 20px;
    font-weight: 500;
    color: #1ebe8c;
}

h5 {
    font-size: 28px;
    font-weight: 300;
    color: #1ebe8c;
    margin-bottom: 0.7rem;
}

p {
    color: #959094;
}

p.lead {
