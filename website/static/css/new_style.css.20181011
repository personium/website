@media (min-width: 992px) {
    .navbar-expand-lg .navbar-nav .nav-link {
        padding-right: 1rem;
        padding-left: 1rem;
        font-size: 14px;
		color: #ffffff;
}
		
    }
    .navbar-nav>.nav-item>.nav-link.active:after {
        content: "";
